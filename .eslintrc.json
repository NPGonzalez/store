{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "rules": {
            "semi": ["error", "always"],
            "quotes": ["error", "double"]
        }
    }
}